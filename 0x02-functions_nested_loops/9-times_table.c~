# include "holberton.h"

/**
 * times_table - accepts void
 *              prints multiplication table to 9
 *              followed by new line
 * Return: alwayse 0
 *
 */



void times_table(void)
{
	int i;
	int j;

	for (i = '0' ; i <= '9' ; i++)
	{
		for (j = '0' ; j <= '9' ; j++)
		{
			_putchar(i * j);
			_putchar(',');
			_putchar(' ');
			_putchar(' ');
		}
		_putchar('\n');
	}
}
